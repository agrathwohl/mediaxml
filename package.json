{
  "name": "mediaxml",
  "version": "0.0.0",
  "description": "A module for working with media manifests represented by XML like ADI, mRSS, and SCTE236.",
  "scripts": {
    "test": "tape tests",
    "docs:server": "documentation serve -w index.js -a public -g -c documentation.yml",
    "docs:build": "npm run docs:build:html && npm run docs:build:md",
    "docs:build:html": "documentation build index.js -a public -f html -g -o docs -c docs/html.yml",
    "docs:build:md": "documentation build index.js -a public -f md -g -o docs/api.md -c docs/md.yml  --markdown-toc-max-depth=4"
  },
  "bin": {
    "mxml": "bin/mxml"
  },
  "keywords": [
    "mrss",
    "adi1",
    "adi3",
    "ansi",
    "scte",
    "236",
    "2017",
    "cable",
    "labs",
    "content",
    "metadata",
    "media",
    "xml",
    "manifest",
    "package"
  ],
  "author": "Joseph Werle <joseph.werle@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@12core/chromafi": "^3.0.0",
    "camelcase": "^6.2.0",
    "date-and-time": "^0.14.2",
    "debug": "^4.3.1",
    "defined": "^1.0.0",
    "highlight.js": "^10.5.0",
    "htmlparser2": "^6.0.0",
    "jsonata": "^1.8.4",
    "mime": "^2.5.0",
    "minimist": "^1.2.5",
    "nanoassert": "^2.0.0",
    "npt-timecode": "^0.1.2",
    "number-to-words": "^1.2.4",
    "smpte-timecode": "^1.2.3",
    "streamx": "^2.10.1",
    "tinyduration": "^3.2.0"
  },
  "devDependencies": {
    "documentation": "^13.1.0",
    "nyc": "^15.1.0",
    "tape": "^5.1.1"
  },
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "example": "example",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/little-core-labs/mediaxml.git"
  },
  "bugs": {
    "url": "https://github.com/little-core-labs/mediaxml/issues"
  },
  "homepage": "https://github.com/little-core-labs/mediaxml#readme"
}
